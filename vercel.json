{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.mts",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.mts"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    }
  ]
}
