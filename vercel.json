{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/**/*.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "dist/api/$1.mjs" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
